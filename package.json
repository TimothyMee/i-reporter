{
  "name": "ireporter",
  "version": "1.0.0",
  "description": "a reporting application",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/*.spec.js --require @babel/register --exit",
    "test-watch": "nodemon --exec 'npm test'",
    "lint": "eslint ./server/**/**.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "start--nodemon": "nodemon ./server/index.js --watch server --exec babel-node",
    "start-dev": "nodemon --exec babel-node server/index.js",
    "coverage": "node_modules/.bin/nyc --require @babel/register node_modules/.bin/mocha test/*.spec.js --exit",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "author": "Timothy-Mee",
  "license": "ISC",
  "dependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "joi": "^14.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-istanbul": "^0.12.2",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  }
}
